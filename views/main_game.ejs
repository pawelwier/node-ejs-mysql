<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" type="text/css" href="/css/main.css">
    <script type="text/javascript" src="/js/game_run.js" defer></script>
    <title>Ruletka</title>
</head>
<body>
    <table id="numBoard">
        <tr>
            <% for (let i = 1; i<13; i++) { %>
                <th>
                    <%= i %>
                </th>
            <% } %>
        </tr>
        <tr>
            <% for (let x of fieldArr) { %>
            <% if  (x.val%3==0&&x.val>0) { %>
                <td>
                    <%= x.val %>
                </td>
            <% }} %>
        </tr>
        <tr>
            <% for (let x of fieldArr) { %>
            <% if  (x.val%3==2) { %>
                <td>
                    <%= x.val %>
                </td>
            <% }} %>
        </tr>
        <tr>
            <% for (let x of fieldArr) { %>
            <% if  (x.val%3==1) { %>
                <td>
                    <%= x.val %>
                </td>
            <% }} %>
        </tr>
        <tr>
            <% for (let x of fieldArr) { %>
            <% if  (x.val==0) { %>
                <td>
                    <%= x.val %>
                </td>
            <% }} %>
        </tr>

    
    </table>

</body>
</html>